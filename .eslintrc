{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "plugins": ["react", "prettier"],
    "rules": {
      "react/prop-types": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [".js", "jsx"]
        }
      ],
      "prettier/prettier": "error",
      "max-len": ["error", 80]
    },
    "env": {
      "browser": true
    }
  }
  